    #write "/sys/devices/platform/soc/soc:qcom,cpu0-llcc-ddr-lat/devfreq/soc:qcom,cpu0-llcc-ddr-lat/governor" performance

    #write "/sys/devices/platform/soc/soc:qcom,cpu-llcc-ddr-bw/devfreq/soc:qcom,cpu-llcc-ddr-bw/governor" performance
    #write "/sys/devices/platform/soc/soc:qcom,cpu-cpu-llcc-bw/devfreq/soc:qcom,cpu-cpu-llcc-bw/governor" performance
    #write "/sys/devices/platform/soc/soc:qcom,npu-npu-ddr-bw/devfreq/soc:qcom,npu-npu-ddr-bw/min_freq" 6881

    #write "/sys/devices/platform/soc/1d84000.ufshc/devfreq/1d84000.ufshc/min_freq" 200000000

    # Setup runtime cpusets
    write /dev/cpuset/top-app/cpus 0-7
    write /dev/cpuset/foreground/cpus 0-5
    #write /dev/cpuset/foreground/boost/cpus 0-7
    write /dev/cpuset/background/cpus 0-5
    write /dev/cpuset/system-background/cpus 0-5
    write /dev/cpuset/restricted/cpus 2-5

    # Setup default schedTune value for foreground/top-app
    write /dev/stune/foreground/schedtune.prefer_idle 1
    write /dev/stune/top-app/schedtune.boost ${persist.bkp.performance.schboost:-5}
    write /dev/stune/top-app/schedtune.prefer_idle 1

    # Setup dynamic stune boost
    # write /sys/module/cpu_boost/parameters/dynamic_stune_boost 15
    # write /sys/module/cpu_boost/parameters/dynamic_stune_boost_ms 500
    # write /dev/stune/top-app/schedtune.sched_boost 15

